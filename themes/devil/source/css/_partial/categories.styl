#categories ul
  padding: 3px 0 3px 30px

#categories>ul
  padding: 3px 0

#categories li
  padding: 3px 0
  list-style-type: none

#categories a
  color: #161209
  transition: color 0.2s ease, border-color 0.2s ease, background 0.2s ease, opacity 0.2s ease
  cursor: pointer

#categories a:hover
  color: #2d96bd
  transition: color 0.2s ease, border-color 0.2s ease, background 0.2s ease, opacity 0.2s ease

#categories span
  padding: 0px 5px
  float: right
